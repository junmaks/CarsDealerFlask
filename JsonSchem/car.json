  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "brand": {
      "type": "string"
    },
    "car_model": {
      "type": "string"
    },
    "year_of_release": {
      "type": "integer"
    },
    "price": {
      "type": "integer"
    },
    "dealer": {
      "type": "integer"
    }
